<%- include ../partials/header %>
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad">
            <div class="row">
                <div class="col-sm-11">
                    <table>
                        <thead>
                            <tr>
                                <th>
                                    Operation
                                </th>

                                <th>
                                    Fornavn
                                </th>

                                <th>
                                    Efternavn
                                </th>
                                <th>
                                    Tlf. nummer
                                </th>
                                <th>
                                    Brugernavn
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <% results.forEach(bruger => {%>
                                <tr>
                                    <td>
                                        <button id=<%=bruger.id %> onclick="entfernenBenutzer(this.id)">Slet</button>
                                    </td>
                                    <td>
                                        <%= bruger.first_name %>
                                    </td>
                                    <td>
                                        <%= bruger.last_name %>
                                    </td>
                                    <td>
                                        <%= bruger.mob_no %>
                                    </td>
                                    <td>
                                        <%= bruger.user_name %>
                                    </td>
                                </tr>
                                <% }) %>
                        </tbody>
                    </table>
                </div>
                <div class="col-sm-1">
                    <a href="http://localhost:3000/signup">
                        <button class="btn">Opret bruger</button>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <%- include ../partials/footer %>